@model IEnumerable<JAMSv1._0.Models.Accomplishment>

@{
    ViewBag.Title = "Index";
}

<h2>Review</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AccomplishmentDate1)
